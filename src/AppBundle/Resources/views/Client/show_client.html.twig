{% extends "::base.html.twig" %}

{% block title %}AppBundle:Client:showClient{% endblock %}

{% block body %}
    {% for client in client %}
    <h1>Welcome to your login page {{ client.email }}</h1>
    <div>
        <div><bold>{{ client.email }}</bold></div>
        <div><bold>{{ client.phone }}</bold></div>
        <div><bold>{{ client.deliveryaddress }}</bold></div>
    </div>
    {% endfor %}
    <div></div>
    <div></div>
{% endblock %}
